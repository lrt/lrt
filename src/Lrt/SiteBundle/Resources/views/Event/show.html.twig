<h1>Informations évènement</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Titre</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Date de début</th>
            <td>{{ entity.dateDeb|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Date de fin</th>
            <td>{{ entity.dateEnd|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Lieux</th>
            <td>{{ entity.place }}</td>
        </tr>
        <tr>
            <th>Website</th>
            <td>{{ entity.website }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
    </tbody>
</table>
