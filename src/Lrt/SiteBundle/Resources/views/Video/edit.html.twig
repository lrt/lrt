{% extends '::admin.html.twig' %}

{% block body %}
<h1>Video edit</h1>

<form action="{{ path('video_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('video') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}